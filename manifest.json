{
    "manifest_version": 3,
    "name": "YouTube AD Skipper",
    "version": "1.0",
    "description": "Skips the YouTube ADs by increasing the playback rate",
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "*://*.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "icons": {
      "128": "./icon.png"
    }
  }